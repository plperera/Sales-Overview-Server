{
  "name": "node-app",
  "version": "1.0.0",
  "description": "A standard backend template using TypeScript, Prisma, and Node.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/server.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prod": "cross-env NODE_ENV=production node dist/server.js",
    "migration:run": "prisma migrate deploy",
    "migration:generate": "prisma migrate dev",
    "test": "jest",
    "clean": "rimraf dist node_modules"
  },
  "author": "Seu Nome",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.11.26",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^28.1.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.18.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
